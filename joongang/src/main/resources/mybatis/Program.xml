<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Program">
	<resultMap id="ProgramResult" type="Program">
	    <result property="upload_num" column="pnum"/>
	    <result property="savefile_name" column="gubun"/>
	    <result property="original_name" column="yymm"/>
	    <result property="reg_date" column="pname"/>
	    <result property="room_num" column="room_num"/>
	    <result property="class_name" column="class_num"/>
	    <result property="class_num" column="class_num"/>
	</resultMap> 
  <insert id="insert2" parameterType="Program">
	 insert into program values (#{upload_num},#{class_num},#{savefile_name},#{original_name},
	 #{reg_date},#{class_name},#{room_num} ) 
  </insert>
  
	 <insert id="insert1" parameterType="Program">
	 insert into program values (#{upload_num},#{class_num},#{savefile_name},#{original_name},
	 #{reg_date},#{class_name},#{room_num} ) 
 	</insert>
 	
    <insert id="class_file" parameterType="Program"> 
     insert into program(upload_num, class_num, class_name, room_num) values ( #{upload_num}, #{class_num}, #{class_name}, #{room_num} )
  	</insert>

</mapper>